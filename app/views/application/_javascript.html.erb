<%= javascript_include_tag '//npmcdn.com/imagesloaded@4.1/imagesloaded.pkgd.min.js' %>
<%= javascript_include_tag '//npmcdn.com/isotope-layout@3.0/dist/isotope.pkgd.min.js' %>

